---
interface Props {
	title: string;
}import { Icon } from 'astro-icon/components';
const { title } = Astro.props;
---

<!doctype html>
<html lang="es">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Astro description" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body>
		<header class="bg-orange-400 h-20"></header>
		<menu class="bg-blue-400 flex">
			<li><a href="/" class="block px-2 hover:bg-white/25">Inicio</a></li>
			<li><a href="/quienes-somos" class="block px-2 hover:bg-white/25">Quienes somos</a></li>
			<li><a href="/organigrama" class="block px-2 hover:bg-white/25">Organigrama</a></li>
			<li><a href="/noticias" class="block px-2 hover:bg-white/25">Noticias</a></li>
			<li><a href="/contacto" class="block px-2 hover:bg-white/25">Conctacto</a></li>
			<Icon name="mdi:elephant" size="100" class="text-blue-600"/>
		</menu>

		<slot/>
	</body>
</html>

<style is:global>
	@font-face {
  font-family: 'myfont';
  src: url('/fonts/Roboto-Regular.ttf');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
</style>

